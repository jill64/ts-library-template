{
  "name": "ts-library-template",
  "description": "⛩️ TypeScript Library Template",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/ts-library-template/issues",
  "homepage": "https://github.com/jill64/ts-library-template#readme",
  "files": [
    "dist",
    "!**/*.test.*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/ts-library-template.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "test": "vitest",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "0.1.0",
    "@jill64/prettier-config": "0.0.1",
    "typescript": "5.3.2",
    "vitest": "0.34.6"
  }
}
